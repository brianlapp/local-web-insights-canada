# Server Configuration
PORT=3002
NODE_ENV=development
API_VERSION=1.0.0

# Logging
LOG_LEVEL=debug

# Supabase Configuration - Update with your existing project details
# You can find these values in your Supabase dashboard > Settings > API
SUPABASE_URL=your_existing_supabase_url
SUPABASE_KEY=your_existing_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_existing_service_role_key

# Authentication
# Note: This should use the same JWT secret that your dashboard uses
# or implement token verification against Supabase auth
JWT_SECRET=local_web_insights_dev_secret
JWT_EXPIRES_IN=24h
API_KEY_HEADER=X-API-Key

# CORS Configuration - Update to include your dashboard URL
CORS_ORIGIN=http://localhost:3000,http://localhost:3001 