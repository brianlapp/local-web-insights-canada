{
  "name": "local-web-insights-api",
  "version": "1.0.0",
  "description": "API Service for Local Web Insights Canada",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
