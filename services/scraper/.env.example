# Server Configuration
PORT=3000
NODE_ENV=development

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_INTERNAL_URL=redis://redis.railway.internal:6379

# Google Maps Configuration
# For a single API key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# For multiple API keys with quotas (recommended for production)
# Format: key1:dailyQuota1,key2:dailyQuota2
# Example: abcdef123:1000,ghijkl456:1500
GOOGLE_MAPS_API_KEYS=key1:1000,key2:1000

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Google Cloud Storage (for screenshots and reports)
GCS_BUCKET_NAME=your_gcs_bucket_name
GCS_CREDENTIALS={"type":"service_account",...}

# MadeWith API (for technology detection)
MADEWITH_API_KEY=your_madewith_api_key

# Logging Configuration
LOG_LEVEL=info # Options: error, warn, info, debug

# Queue Configuration
QUEUE_CONCURRENCY=3
QUEUE_LIMITER_MAX=5
QUEUE_LIMITER_DURATION=10000

# Circuit Breaker Configuration
CIRCUIT_BREAKER_TIMEOUT=60000
MAX_RETRIES=5
RETRY_DELAY=2000 